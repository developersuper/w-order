{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Literate, Serilog.Sinks.Console",
          "outputTemplate": "[{RequestId} {Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "MenublyDatabase": ""
  },
  "SendGrid": {
    "ResetPasswordClientUrl": "https://dev.menubly.com/reset-password?code={0}",
    "ResetPasswordEmailTemplate": "https://menubly-public-dev.s3.ap-southeast-1.amazonaws.com/email-templates/menubly-reset-password-template.html",
    "TemplateCacheDurationInHour": 12
  },
  "App": {
    "CorsOrigins": "https://*.menubly.com,http://localhost:3000,https://localhost:7124",
    "BaseUrl": "https://dev.menubly.com",
    "PublicBucketName": "menubly-public-dev",
    "PublicStorageServerUrl": "https://d1oatbla6tt5qo.cloudfront.net",
    "MaxItemPerPlace": 20,
    "MaxPlacePerAccount": 5
  },
  "Sentry": {
    "Dsn": "",
    "MaxRequestBodySize": "Always",
    "SendDefaultPii": true,
    "MinimumBreadcrumbLevel": "Debug",
    "MinimumEventLevel": "Warning",
    "AttachStackTrace": true,
    "Debug": true,
    "DiagnosticsLevel": "Error",
    "TracesSampleRate": 0.1
  }
}
